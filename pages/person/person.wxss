  position: fixed;
  bottom: 0;
  width: 100%;
  border-top: 1rpx solid #6a6a6a;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 10pt;



}
.price{
  display:flex;
justify-content:flex-end;
width:68%;

}
.deg{
  position: fixed;
  bottom: 0;
  width: 100%;
}
.button{
  border: 1rpx solid #6a6a6a;
  border-radius: 0;
  background: white;
  outline: 0;
  font-size: 14pt;
  height: 100rpx;
  color: #333;
  width: 100%;
}

button[class="button"]::after {
  border: 0;
}